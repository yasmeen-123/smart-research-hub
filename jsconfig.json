{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "jsx": "react-jsx",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "types": []
    },
    "typeAcquisition": {
        "exclude": ["caniuse-lite", "electron-to-chromium"]
    },
    "include": ["frontend/**/*"],
    "exclude": ["node_modules", "**/node_modules/*"]
}